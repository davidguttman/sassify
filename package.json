{
  "name": "sassify",
  "version": "0.4.0",
  "description": "Browserify middleware for adding required styles to the page.",
  "main": "index.js",
  "scripts": {
    "test": "browserify -t . -r ./index.js:sassify test/*.js | testling"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/davidguttman/sassify.git"
  },
  "keywords": [
    "browserify",
    "css",
    "middleware"
  ],
  "browser": "./lib/sassify-browser.js",
  "author": "David Guttman",
  "license": "BSD-2-Clause",
  "dependencies": {
    "cssify": "^0.5.0",
    "node-sass": "^0.8.4",
    "through": "^2.3.4"
  },
  "devDependencies": {
    "tape": "^2.12.3",
    "testling": "^1.6.1"
  },
  "bugs": {
    "url": "https://github.com/davidguttman/sassify/issues"
  },
  "homepage": "https://github.com/davidguttman/sassify",
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
